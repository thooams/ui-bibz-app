(function() {
  var Interface;

  this.UiBibzInterface = Interface = (function() {
    function Interface() {
      this.nav();
      this.tooltip();
      this.dropdown();
    }

    Interface.prototype.nav = function() {
      return $('.nav-tabs a').click(function(e) {
        e.preventDefault();
        return $(this).tab('show');
      });
    };

    Interface.prototype.tooltip = function() {
      return $('[data-toggle="tooltip"]').tooltip();
    };

    Interface.prototype.dropdown = function() {
      return $('.dropdown-toggle').dropdown();
    };

    return Interface;

  })();

}).call(this);
